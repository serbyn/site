	doi = {10.1126/science.1239451},
	issn = {0036-8075},
	journal = {Science},
	number = {6153},
	pages = {1496--1499},
	publisher = {American Association for the Advancement of Science},
	title = {Observation of {Dirac} Node Formation and Mass Acquisition in a Topological Crystalline Insulator},
	url = {https://science.sciencemag.org/content/341/6153/1496},
	volume = {341},
	year = {2013},
	bdsk-url-1 = {https://science.sciencemag.org/content/341/6153/1496},
